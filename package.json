{
  "name": "PBSDex",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all -p dev:client dev:astro",
    "dev:astro": "astro dev",
    "dev:client": "esbuild src/scripts/main.ts --bundle --format=esm --outfile=public/PBSDex/js/main.js --watch",
    "build": "npm run build:client && astro build",
    "build:client": "esbuild src/scripts/main.ts --bundle --format=esm --outfile=public/PBSDex/js/main.js"
  },
  "dependencies": {
    "astro": "^5.13.5"
  },
  "devDependencies": {
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5"
  }
}
